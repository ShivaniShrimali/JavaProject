package com.agecalculator.servlet;

import java.io.IOException;
import java.io.PrintWriter;
import java.sql.Date;
import java.text.ParseException;
import java.text.SimpleDateFormat;
import java.time.LocalDate;

import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

@WebServlet("/getdata")
public class AgeCalcuDemo extends HttpServlet {
	private static final long serialVersionUID = 1L;
       
  

	protected void doGet(HttpServletRequest request, HttpServletResponse response) throws ServletException, IOException {

		response.setContentType("text/html");
		PrintWriter pw = response.getWriter();

		String starDatebirth=request.getParameter("dateofbirth");

//		String starDatecurrent=request.getParameter("currentdate");
		SimpleDateFormat sdf=new SimpleDateFormat("dd/MM/yyyy");
		
		try{
			Date startDate=(Date) sdf.parse(starDatebirth);
			System.out.println(startDate.getMonth(););
			System.out.println(startDate.getDay());
			
			System.out.println(startDate.getYear());startDate.getYear();
		
		} catch (ParseException e) {
		
			e.printStackTrace();
		}
			
	}

}
